  {
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "env": {
      "DB_USER": "@db_user",
      "DB_HOST": "@db_host",
      "DB_NAME": "@db_name",
      "DB_PASSWORD": "@db_password",
      "DB_PORT": "@db_port"
    }
  }